{
    "semi": false,
    "trailingComma": "none",
    "singleQuote": true,
    "printWidth": 140,
    "arrowParens": "avoid",
    "plugins": [
        "prettier-plugin-svelte"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}
